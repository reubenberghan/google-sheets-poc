{
  "name": "google-sheets-poc",
  "private": true,
  "description": "A simple POC for using Google Sheets with Gatsby",
  "version": "0.1.0",
  "author": "Reuben Berghan <reuben.berghan@gmail.com>",
  "dependencies": {
    "gatsby": "^4.25.7",
    "gatsby-image": "^2.0.20",
    "gatsby-plugin-manifest": "^2.0.9",
    "gatsby-plugin-offline": "^2.0.16",
    "gatsby-plugin-react-helmet": "^3.0.2",
    "gatsby-plugin-sharp": "^2.0.14",
    "gatsby-source-filesystem": "^2.0.8",
    "gatsby-source-google-sheets": "^1.1.1",
    "gatsby-transformer-sharp": "^2.1.8",
    "prop-types": "^15.6.2",
    "ramda": "^0.26.1",
    "ramda-adjunct": "^2.13.0",
    "react": "16.8.0-alpha.0",
    "react-dom": "16.8.0-alpha.0",
    "react-helmet": "^5.2.0",
    "styled-components": "^4.1.3",
    "styled-normalize": "^8.0.4"
  },
  "keywords": [
    "gatsby",
    "google sheets"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "start": "npm run develop",
    "format": "standard 'src/**/*.js' --fix",
    "test": "echo \"Write tests! -> https://gatsby.app/unit-testing\""
  },
  "devDependencies": {
    "dotenv": "^6.2.0",
    "husky": "^1.3.1",
    "import-sort": "^5.2.0",
    "import-sort-style-airkro": "^0.1.6",
    "lint-staged": "^8.1.0",
    "prettier": "^1.15.2",
    "standard": "^12.0.1"
  },
  "eslintConfig": {
    "extends": "react-app",
    "globals": {
      "toJson": false
    }
  },
  "importSort": {
    ".js": {
      "parser": "babylon",
      "style": "airkro"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.js": [
      "standard --fix",
      "git add"
    ]
  },
  "standard": {
    "env": [
      "jest"
    ],
    "globals": [
      "fetch",
      "toJson"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/reubenberghan/google-sheets-poc"
  },
  "bugs": {
    "url": "https://github.com/reubenberghan/google-sheets-poc/issues"
  }
}
